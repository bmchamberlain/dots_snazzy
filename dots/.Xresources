urxvt.scrollBar: false

urxvt.depth: 32

urxvt.keysym.Control-Shift-V: perl:clipboard:paste
urxvt.keysym.Control-Shift-C: perl:clipboard:copy

urxvt.iso14755: False

urxvt.perl-ext-common: default,clipboard

!Fonts

urxvt.font: xft:Ubuntu Mono:pixelsize=17,style=regular, xft:Menlo for Powerline:pixelsize=17,style=regular 
urxvt.boldFont: xft:Ubuntu Mono:pixelsize=17,style=bold
urxvt.letterSpace: -1


Xft.dpi: 96	
Xft.antialias: true
Xft.hintstyle: hintfull
Xft.hinting: true

*.internalBorder: 13



